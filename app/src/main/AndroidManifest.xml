<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.app.dharm.info.online.dharmadmin">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DharmAdmin"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.AddNewCoupansActivity"></activity>
        <activity android:name=".activity.AddNewBannerActivity" />
        <activity android:name=".activity.ShowAllUsersActivity" />
        <activity android:name=".activity.ShowAllRequestedDealsActivity" />
        <activity android:name=".activity.ShowAllFeedbackActivity" />
        <activity android:name=".activity.ShowAllOrdersActivity" />
        <activity android:name=".activity.AddNewProductActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_channel_id"
            android:value="@string/default_notification_channel_id" />
    </application>

</manifest>